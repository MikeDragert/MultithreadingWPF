<Window x:Class="MultithreadingWPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MultithreadingWPF"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
    <TextBox HorizontalAlignment="Left" Height="198" Margin="155,107,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="905" Text="{Binding ResultBoxText, UpdateSourceTrigger=PropertyChanged}"/>
    <Button x:Name="ExecuteButton" Content="Execute" Height="69" Margin="155,352,0,0" VerticalAlignment="Top" Width="291" Command="{Binding ExecuteCommand}"/>

  </Grid>
</Window>
